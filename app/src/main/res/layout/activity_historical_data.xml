<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
        android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        tools:context="com.kasct.pnu.massari.HistoricalData"
        android:background="@drawable/background"
        >

    <TextView
            android:layout_width="30dp"
            android:layout_height="398dp"
            android:id="@+id/textView"
            android:background="#38ffffff"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="رقم المسار"
            android:id="@+id/textView6"
            android:textSize="24dp"
            android:textColor="@color/Black"
            android:layout_alignTop="@+id/textView"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="رقم المركبة"
            android:id="@+id/textView7"
            android:textSize="24dp"
            android:textColor="@color/Black"
            android:layout_below="@+id/textView6"
            android:layout_alignRight="@+id/textView"
            android:layout_alignEnd="@+id/textView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="الإسم"
            android:id="@+id/textView8"
            android:textColor="@color/Black"
            android:textSize="24dp"
            android:layout_alignRight="@+id/textView7"
            android:layout_alignEnd="@+id/textView7"
            android:layout_below="@+id/textView7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="التاريخ"
            android:id="@+id/textView9"
            android:textColor="@color/Black"
            android:textSize="24dp"
            android:layout_below="@+id/textView8"
            android:layout_alignLeft="@+id/textView6"
            android:layout_alignStart="@+id/textView6"
            android:layout_alignRight="@+id/textView"
            android:layout_alignEnd="@+id/textView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="وقت بداية التعقب"
            android:id="@+id/textView10"
            android:textColor="@color/Black"
            android:textSize="24dp"
            android:layout_below="@+id/textView9"
            android:layout_alignRight="@+id/textView"
            android:layout_alignEnd="@+id/textView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="وقت نهاية التعقب"
            android:id="@+id/textView11"
            android:textColor="@color/Black"
            android:textSize="24dp"
            android:layout_below="@+id/textView10"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="المسار"
            android:id="@+id/textView18"
            android:textColor="@color/Black"
            android:textSize="24dp"
            android:layout_below="@+id/textView11"
            android:layout_alignRight="@+id/textView"
            android:layout_alignEnd="@+id/textView"
            android:layout_alignWithParentIfMissing="true" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="عرض المسار"
            android:id="@+id/button3"
            android:backgroundTint="#ebcddc39"
            android:textStyle="normal|bold"
            android:textSize="24dp"
            android:textColor="@color/LightWhite"
            android:layout_alignBottom="@+id/textView"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

    </RelativeLayout>

